<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="1column" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd" label="Multishipping Checkout" design_abstraction="custom">
	<referenceBlock name="breadcrumbs">
		<action method="addCrumb">
			<argument name="crumbName" xsi:type="string" translate="true">Home</argument>
			<argument name="crumbInfo" xsi:type="array">
				<item name="title" xsi:type="string" translate="true">Home</item>
				<item name="label" xsi:type="string" translate="true">Home</item>
				<item name="link" xsi:type="string" translate="true">{{baseUrl}}</item>
			</argument>
		</action>
		<action method="addCrumb">
			<argument name="crumbName" xsi:type="string" translate="true">Check Out with Multiple Addresses</argument>
			<argument name="crumbInfo" xsi:type="array">
				<item name="title" xsi:type="string" translate="true">Check Out with Multiple Addresses</item>
				<item name="label" xsi:type="string" translate="true">Check Out with Multiple Addresses</item>
			</argument>
		</action>
	</referenceBlock>
</page>
