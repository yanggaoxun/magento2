<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
	<acl>
        <resources>
			<resource id="Magento_Backend::admin">
				<resource id="Magento_Backend::mgs">
					<resource id="MGS_Protabs::main" title="Product Tabs" sortOrder="90">
						<resource id="MGS_Protabs::manage" title="Tabs Setting" sortOrder="10"/>
					</resource>
				</resource>
				
				<resource id="Magento_Backend::stores">
                    <resource id="Magento_Backend::stores_settings">
                        <resource id="Magento_Config::config">
							<resource id="MGS_Mpanel::mpanel">
								<resource id="MGS_Protabs::config_protabs" title="Product Tabs" sortOrder="100"/>
							</resource>
                        </resource>
                    </resource>
                </resource>
            </resource>
        </resources>
    </acl>
</config>
