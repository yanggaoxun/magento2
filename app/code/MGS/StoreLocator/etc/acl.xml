<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">
				<resource id="Magento_Backend::mgs">
					<resource id="MGS_StoreLocator::storelocator" title="Store Locator" sortOrder="40">
						<resource id="MGS_StoreLocator::manage" title="Manage Stores" sortOrder="10"/>
					</resource>
				</resource>
				<resource id="Magento_Backend::stores">
                    <resource id="Magento_Backend::stores_settings">
                        <resource id="Magento_Config::config">
							<resource id="MGS_Mpanel::mpanel">
								<resource id="MGS_StoreLocator::locator_manage" title="Store Locator" sortOrder="40"/>
							</resource>
                        </resource>
                    </resource>
                </resource>
            </resource>
        </resources>
    </acl>
</config>
