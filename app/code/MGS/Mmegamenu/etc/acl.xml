<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
    <acl>
        <resources>
			<resource id="Magento_Backend::admin">
				<resource id="Magento_Backend::mgs">
					<resource id="MGS_Mmegamenu::megamenu" title="Megamenu" sortOrder="10">
						<resource id="MGS_Mmegamenu::parent" title="Manage Menu" sortOrder="10" />
						<resource id="MGS_Mmegamenu::items" title="Manage Megamenu Items" sortOrder="20" />
					</resource>
				</resource>
				
				<resource id="Magento_Backend::stores">
                    <resource id="Magento_Backend::stores_settings">
                        <resource id="Magento_Config::config">
							<resource id="MGS_Mpanel::mpanel">
								<resource id="MGS_Mmegamenu::config_megamenu" title="Megamenu Setting" sortOrder="20" />
							</resource>
                        </resource>
                    </resource>
                </resource>
            </resource>
        </resources>
    </acl>
</config>
